<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="MSSqlConStr" connectionString="Data Source=(local);Initial Catalog=Test2;Integrated Security=SSPI;" />
    <add name="TestBulankin.Properties.Settings.TestConnectionString"
      connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\Test.mdf;Integrated Security=True"
